{
  "name": "websocket-presentator",
  "version": "1.0.0",
  "description": "A simple websockets server/client for presentation purposes",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.js",
    "build-css": "node-sass --include-path scss src/index.scss src/index.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "deploy": "npx now"
  },
  "author": "Mauricio Palma <mauricio.palma@sinnerschrader.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.15.3",
    "http": "0.0.0",
    "now": "^7.2.4",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "ambient-light-sensor": "0.0.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0"
  }
}
